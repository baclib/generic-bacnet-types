{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "title": "Unsigned Range Constraint",
    "type": "object",
    "properties": {
        "range": {
            "type": "object",
            "properties": {
                "minimum": {
                    "type": "integer",
                    "minimum": 0
                },
                "maximum": {
                    "type": "integer",
                    "minimum": 0
                }
            },
            "anyOf": [
                { "required": ["minimum"] },
                { "required": ["maximum"] }
            ],
            "additionalProperties": false
        }
    }
}
