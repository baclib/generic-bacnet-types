{
    "alias": "BACnetSpecialEvent",
    "name": "special-event",
    "type": {
        "base": "sequence",
        "fields": [
            {
                "name": "period",
                "type": {
                    "base": "choice",
                    "options": [
                        {
                            "name": "calendar-entry",
                            "type": "calendar-entry",
                            "context": 0
                        },
                        {
                            "name": "calendar-reference",
                            "type": "object-identifier",
                            "context": 1
                        }
                    ]
                }
            },
            {
                "name": "list-of-time-values",
                "type": {
                    "base": "time-value",
                    "series": true
                },
                "context": 2
            },
            {
                "name": "event-priority",
                "type": {
                    "base": "unsigned",
                    "minimum": 1,
                    "maximum": 16
                },
                "context": 3
            }
        ]
    }
}