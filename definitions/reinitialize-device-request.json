{
    "alias": "ReinitializeDevice-Request",
    "name": "reinitialize-device-request",
    "description": "Represents a BACnet ReinitializeDevice request, used to reinitialize a device to a specified state, optionally with a password.",
    "type": {
        "base": "sequence",
        "fields": [
            {
                "name": "reinitialized-state-of-device",
                "type": {
                    "base": "enumerated",
                    "values": [
                        {
                            "name": "coldstart",
                            "constant": 0,
                            "description": "Device will perform a coldstart."
                        },
                        {
                            "name": "warmstart",
                            "constant": 1,
                            "description": "Device will perform a warmstart."
                        },
                        {
                            "name": "start-backup",
                            "constant": 2,
                            "description": "Device will start backup operation."
                        },
                        {
                            "name": "end-backup",
                            "constant": 3,
                            "description": "Device will end backup operation."
                        },
                        {
                            "name": "start-restore",
                            "constant": 4,
                            "description": "Device will start restore operation."
                        },
                        {
                            "name": "end-restore",
                            "constant": 5,
                            "description": "Device will end restore operation."
                        },
                        {
                            "name": "abort-restore",
                            "constant": 6,
                            "description": "Device will abort restore operation."
                        },
                        {
                            "name": "activate-changes",
                            "constant": 7,
                            "description": "Device will activate configuration changes."
                        }
                    ]
                },
                "context": 0,
                "description": "The state to which the device should be reinitialized."
            },
            {
                "name": "password",
                "type": {
                    "base": "character-string",
                    "length": {
                        "minimum": 1,
                        "maximum": 20
                    }
                },
                "context": 1,
                "optional": true,
                "description": "Optional password required for reinitialization."
            }
        ]
    }
}