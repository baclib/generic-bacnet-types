    "alias": "BACnet-ComplexACK-PDU",
    "name": "complex-ack-pdu",
    "description": "Represents a BACnet Complex Acknowledgment Protocol Data Unit (PDU) for confirmed services that return data.",
    "type": {
        "base": "sequence",
        "fields": [
            {
                "name": "pdu-type",
                "type": {
                    "base": "unsigned",
                    "minimum": 0,
                    "maximum": 15
                },
                "context": 0,
                "description": "The PDU type identifier (3 for BACnet-ComplexACK-PDU)."
            },
            {
                "name": "segmented-message",
                "type": "boolean",
                "context": 1,
                "description": "Indicates whether this is a segmented message."
            },
            {
                "name": "more-follows",
                "type": "boolean",
                "context": 2,
                "description": "Indicates whether more segments follow this one."
            },
            {
                "name": "reserved",
                "type": {
                    "base": "unsigned",
                    "minimum": 0,
                    "maximum": 3
                },
                "context": 3,
                "description": "Reserved field, must be zero."
            },
            {
                "name": "invoke-id",
                "type": {
                    "base": "unsigned",
                    "minimum": 0,
                    "maximum": 255
                },
                "context": 4,
                "description": "The invoke ID matching the original request."
            },
            {
                "name": "sequence-number",
                "type": {
                    "base": "unsigned",
                    "minimum": 0,
                    "maximum": 255
                },
                "context": 5,
                "optional": true,
                "description": "The sequence number for segmented messages. Optional."
            },
            {
                "name": "proposed-window-size",
                "type": {
                    "base": "unsigned",
                    "minimum": 1,
                    "maximum": 127
                },
                "context": 6,
                "optional": true,
                "description": "The proposed window size for segmented messages. Optional."
            },
            {
                "name": "service-ack-choice",
                "type": "confirmed-service-choice",
                "context": 7,
                "description": "The confirmed service being acknowledged."
            },
            {
                "name": "service-ack",
                "type": "confirmed-service-ack",
                "context": 8,
                "description": "The service acknowledgment data."
            }
        ]
    }
}
