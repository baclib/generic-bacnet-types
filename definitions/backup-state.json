    "alias": "BACnetBackupState",
    "name": "backup-state",
    "description": "Enumeration defining the operational state of a backup or restore operation.",
    "type": {
        "base": "enumerated",
        "values": [
            {
                "name": "idle",
                "constant": 0,
                "description": "Idle state."
            },
            {
                "name": "preparing-for-backup",
                "constant": 1,
                "description": "Preparing for backup state."
            },
            {
                "name": "preparing-for-restore",
                "constant": 2,
                "description": "Preparing for restore state."
            },
            {
                "name": "performing-a-backup",
                "constant": 3,
                "description": "Performing a backup state."
            },
            {
                "name": "performing-a-restore",
                "constant": 4,
                "description": "Performing a restore state."
            },
            {
                "name": "backup-failure",
                "constant": 5,
                "description": "Backup failure state."
            },
            {
                "name": "restore-failure",
                "constant": 6,
                "description": "Restore failure state."
            }
        ]
    }
}
