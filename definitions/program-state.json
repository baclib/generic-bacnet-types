    "alias": "BACnetProgramState",
    "name": "program-state",
    "description": "Enumerates BACnet program states, indicating the current state of a program object.",
    "type": {
        "base": "enumerated",
        "values": [
            {
                "name": "idle",
                "constant": 0,
                "description": "Program is idle."
            },
            {
                "name": "loading",
                "constant": 1,
                "description": "Program is loading."
            },
            {
                "name": "running",
                "constant": 2,
                "description": "Program is running."
            },
            {
                "name": "waiting",
                "constant": 3,
                "description": "Program is waiting."
            },
            {
                "name": "halted",
                "constant": 4,
                "description": "Program is halted."
            },
            {
                "name": "unloading",
                "constant": 5,
                "description": "Program is unloading."
            }
        ]
    }
