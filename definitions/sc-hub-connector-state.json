{
    "alias": "BACnetSCHubConnectorState",
    "name": "sc-hub-connector-state",
    "description": "Enumerates the connector states for BACnet Secure Connect (SC) hubs, indicating the current hub connection.",
    "type": {
        "base": "enumerated",
        "values": [
            {
                "name": "no-hub-connection",
                "constant": 0,
                "description": "No connection to any hub is established."
            },
            {
                "name": "connected-to-primary",
                "constant": 1,
                "description": "Connected to the primary hub."
            },
            {
                "name": "connected-to-failover",
                "constant": 2,
                "description": "Connected to the failover hub."
            }
        ]
    }
}