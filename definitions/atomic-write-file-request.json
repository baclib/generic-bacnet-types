{
    "alias": "AtomicWriteFile-Request",
    "name": "atomic-write-file-request",
    "type": {
        "base": "sequence",
        "fields": [
            {
                "name": "file-identifier",
                "type": "object-identifier"
            },
            {
                "name": "access-method",
                "type": {
                    "base": "choice",
                    "options": [
                        {
                            "name": "stream-access",
                            "type": {
                                "base": "sequence",
                                "fields": [
                                    {
                                        "name": "file-start-position",
                                        "type": "integer"
                                    },
                                    {
                                        "name": "file-data",
                                        "type": "octet-string"
                                    }
                                ]
                            },
                            "context": 0
                        },
                        {
                            "name": "record-access",
                            "type": {
                                "base": "sequence",
                                "fields": [
                                    {
                                        "name": "file-start-record",
                                        "type": "integer"
                                    },
                                    {
                                        "name": "record-count",
                                        "type": "unsigned"
                                    },
                                    {
                                        "name": "file-record-data",
                                        "type": {
                                            "base": "octet-string",
                                            "series": true
                                        }
                                    }
                                ]
                            },
                            "context": 1
                        }
                    ]
                }
            }
        ]
    }
}