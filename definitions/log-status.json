{
    "alias": "BACnetLogStatus",
    "name": "log-status",
    "description": "Represents BACnet Log Status bit string, which indicates the operational state and condition of a logging object through individual status bits.",
    "type": {
        "base": "bit-string",
        "bits": [
            {
                "name": "log-disabled",
                "position": 0,
                "description": "Log is disabled."
            },
            {
                "name": "buffer-purged",
                "position": 1,
                "description": "Log buffer has been purged."
            },
            {
                "name": "log-interrupted",
                "position": 2,
                "description": "Log was interrupted."
            }
        ]
    }
}