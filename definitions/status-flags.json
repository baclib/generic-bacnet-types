    "alias": "BACnetStatusFlags",
    "name": "status-flags",
    "description": "Represents the BACnet status flags, a bit string indicating alarm, fault, override, and out-of-service states.",
    "type": {
        "base": "bit-string",
        "bits": [
            {
                "name": "in-alarm",
                "position": 0,
                "description": "Object is in alarm."
            },
            {
                "name": "fault",
                "position": 1,
                "description": "Object is in fault condition."
            },
            {
                "name": "overridden",
                "position": 2,
                "description": "Object is overridden."
            },
            {
                "name": "out-of-service",
                "position": 3,
                "description": "Object is out of service."
            }
        ]
    }
}
