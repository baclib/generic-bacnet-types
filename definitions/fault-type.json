    "alias": "BACnetFaultType",
    "name": "fault-type",
    "description": "Represents BACnet Fault Type enumeration, which specifies the type of fault condition detected in an object.",
    "type": {
        "base": "enumerated",
        "values": [
            {
                "name": "none",
                "constant": 0,
                "description": "No fault."
            },
            {
                "name": "fault-characterstring",
                "constant": 1,
                "description": "Fault in characterstring."
            },
            {
                "name": "fault-extended",
                "constant": 2,
                "description": "Extended fault."
            },
            {
                "name": "fault-life-safety",
                "constant": 3,
                "description": "Life safety fault."
            },
            {
                "name": "fault-state",
                "constant": 4,
                "description": "State fault."
            },
            {
                "name": "fault-status-flags",
                "constant": 5,
                "description": "Status flags fault."
            },
            {
                "name": "fault-out-of-range",
                "constant": 6,
                "description": "Out of range fault."
            },
            {
                "name": "fault-listed",
                "constant": 7,
                "description": "Listed fault."
            }
        ]
    }
}
