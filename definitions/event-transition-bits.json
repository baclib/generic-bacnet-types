    "alias": "BACnetEventTransitionBits",
    "name": "event-transition-bits",
    "description": "Represents BACnet Event Transition Bits, which indicate the event state transitions that are being monitored, enabled, or acknowledged in event detection.",
    "type": {
        "base": "bit-string",
        "bits": [
            {
                "name": "to-offnormal",
                "position": 0,
                "description": "Transition to offnormal state."
            },
            {
                "name": "to-fault",
                "position": 1,
                "description": "Transition to fault state."
            },
            {
                "name": "to-normal",
                "position": 2,
                "description": "Transition to normal state."
            }
        ]
    }
}
