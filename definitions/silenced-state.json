{
    "alias": "BACnetSilencedState",
    "name": "silenced-state",
    "description": "Enumerates the silenced states for BACnet alarms, indicating which alarm types are silenced.",
    "type": {
        "base": "enumerated",
        "values": [
            {
                "name": "unsilenced",
                "constant": 0,
                "description": "Neither audible nor visible alarms are silenced."
            },
            {
                "name": "audible-silenced",
                "constant": 1,
                "description": "Audible alarms are silenced."
            },
            {
                "name": "visible-silenced",
                "constant": 2,
                "description": "Visible alarms are silenced."
            },
            {
                "name": "all-silenced",
                "constant": 3,
                "description": "Both audible and visible alarms are silenced."
            }
        ],
        "maximum": 65535,
        "proprietary": {
            "from": 64,
            "to": 65535
        }
    }
}