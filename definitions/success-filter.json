{
    "alias": "BACnetSuccessFilter",
    "name": "success-filter",
    "description": "Enumerates the possible filters for BACnet results, allowing selection of all, only successes, or only failures.",
    "type": {
        "base": "enumerated",
        "values": [
            {
                "name": "all",
                "constant": 0,
                "description": "Include all results, both successes and failures."
            },
            {
                "name": "successes-only",
                "constant": 1,
                "description": "Include only successful results."
            },
            {
                "name": "failures-only",
                "constant": 2,
                "description": "Include only failed results."
            }
        ]
    }
}