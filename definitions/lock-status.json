    "alias": "BACnetLockStatus",
    "name": "lock-status",
    "description": "Represents BACnet Lock Status enumeration, which indicates the current state of a physical lock mechanism in access control systems.",
    "type": {
        "base": "enumerated",
        "values": [
            {
                "name": "locked",
                "constant": 0,
                "description": "Lock is engaged."
            },
            {
                "name": "unlocked",
                "constant": 1,
                "description": "Lock is disengaged."
            },
            {
                "name": "lock-fault",
                "constant": 2,
                "description": "Lock fault detected."
            },
            {
                "name": "unused",
                "constant": 3,
                "description": "Lock status is unused."
            },
            {
                "name": "unknown",
                "constant": 4,
                "description": "Lock status is unknown."
            }
        ]
    }
}
