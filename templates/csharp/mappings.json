{
    "fileExtension": ".cs",
    "namespaceTemplate": "namespace {namespace};",
    "namingConventions": {
        "type": "PascalCase",
        "field": "PascalCase",
        "enum": "PascalCase"
    },
    "types": {
        "null": "Null",
        "boolean": "bool",
        "unsigned": "uint",
        "unsigned-8": "byte",
        "unsigned-16": "ushort",
        "unsigned-32": "uint",
        "unsigned-64": "ulong",
        "integer": "int",
        "integer-8": "sbyte",
        "integer-16": "short",
        "integer-32": "int",
        "integer-64": "long",
        "real": "float",
        "double": "double",
        "octet-string": "OctetString",
        "character-string": "CharacterString",
        "string": "string",
        "bit-string": "BitString",
        "bit-string-8": "BitString8",
        "bit-string-16": "BitString16",
        "bit-string-32": "BitString32",
        "bit-string-64": "BitString64",
        "date-pattern": "DatePattern",
        "time-pattern": "TimePattern",
        "object-identifier": "ObjectIdentifier",
        "week-n-day": "WeekNDay"
    }
}